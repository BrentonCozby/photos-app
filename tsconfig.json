{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "types": [
      "Node"
    ],
    "noEmit": true,
    "importHelpers": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true
  }
}
