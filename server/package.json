{
  "name": "photos-app-server",
  "version": "1.0.0",
  "description": "Server for the Photos App",
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register index.ts",
    "start:dev": "NODE_ENV=development nodemon --ext 'js,ts,json' --swc -r tsconfig-paths/register index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "npm run lint",
    "postinstall": "npm run prisma:generate",
    "prisma:migrate": "dotenv -e ../.env -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e ../.env -- npx prisma generate",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "prisma:format": "dotenv -e ../.env -- npx prisma format",
    "test": "jest ./",
    "test:watch": "jest ./ --watch"
  },
  "author": "Brenton Cozby",
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.120.0",
    "@braintree/sanitize-url": "^6.0.0",
    "@prisma/client": "^4.2.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/json-api-serializer": "^2.6.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.38",
    "@types/sanitize-html": "^2.6.2",
    "@types/stoppable": "^1.1.1",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv-cli": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "helmet": "^5.1.0",
    "jimp": "^0.16.1",
    "json-api-serializer": "^2.6.6",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "sanitize-html": "^2.7.0",
    "sharp": "^0.31.0",
    "stoppable": "^1.1.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.2",
    "@swc/helpers": "^0.4.11",
    "@types/jest": "^27.5.2",
    "@types/jimp": "^0.2.28",
    "@types/multer": "^1.4.7",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prisma": "^4.2.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3"
  }
}
