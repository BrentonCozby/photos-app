{
  "name": "photos-app-server",
  "version": "1.0.0",
  "description": "Server for the Photos App",
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register index.ts",
    "start:dev": "NODE_ENV=development nodemon --ext 'js,ts,json' --swc -r tsconfig-paths/register index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "npm run lint",
    "postinstall": "npm run prisma:generate",
    "prisma:migrate": "dotenv -e ../.env -- npx prisma migrate dev",
    "prisma:validate": "dotenv -e ../.env -- npx prisma validate",
    "prisma:generate": "dotenv -e ../.env -- npx prisma generate",
    "prisma:db-pull": "dotenv -e ../.env -- npx prisma db pull",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "prisma:format": "dotenv -e ../.env -- npx prisma format",
    "test": "jest ./",
    "test:watch": "jest ./ --watch"
  },
  "author": "Brenton Cozby",
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.198.0",
    "@braintree/sanitize-url": "^6.0.1",
    "@prisma/client": "^4.5.0",
    "@swc/core": "^1.3.11",
    "@swc/helpers": "^0.4.12",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/json-api-serializer": "^2.6.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.11.7",
    "@types/sanitize-html": "^2.6.2",
    "@types/stoppable": "^1.1.1",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv-cli": "^6.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-oauth2-jwt-bearer": "^1.2.0",
    "helmet": "^6.0.0",
    "jimp": "^0.16.2",
    "json-api-serializer": "^2.6.6",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "prisma": "^4.5.0",
    "sanitize-html": "^2.7.3",
    "sharp": "^0.31.1",
    "stoppable": "^1.1.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/multer": "^1.4.7",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3"
  }
}
