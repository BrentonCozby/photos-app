{
  "name": "photos-app-server",
  "version": "1.0.0",
  "description": "Server for the Photos App",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "start:dev": "nodemon --ext 'js,ts,json' --swc -r tsconfig-paths/register index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "npm run lint",
    "postinstall": "npm run prisma:generate",
    "prisma:migrate": "dotenv -e ../.env -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e ../.env -- npx prisma generate",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "test": "jest ./",
    "test:watch": "jest ./ --watch"
  },
  "author": "Brenton Cozby",
  "engines": {
    "node": "^14 || ^16"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.0",
    "@prisma/client": "^3.10.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/json-api-serializer": "^2.6.3",
    "@types/node": "^17.0.21",
    "@types/sanitize-html": "^2.6.2",
    "@types/stoppable": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^5.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "json-api-serializer": "^2.6.6",
    "pg": "^8.7.3",
    "prisma": "^3.10.0",
    "sanitize-html": "^2.7.0",
    "statuses": "^2.0.1",
    "stoppable": "^1.1.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@swc/core": "^1.2.152",
    "@swc/helpers": "^0.3.6",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3"
  }
}
